<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>

<BorderPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ListView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <HBox prefHeight="38.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <TilePane hgap="30.0" prefHeight="20.0" prefWidth="602.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="HomePagePostsButton" mnemonicParsing="false" text="Posts" />
            <Button fx:id="HomePageJobsButton" mnemonicParsing="false" text="Jobs" />
            <Button fx:id="HomePagePeopleButton" mnemonicParsing="false" text="People" />
            <Button fx:id="HomePageSearchButton" mnemonicParsing="false" text="Search" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets left="70.0" />
         </padding>
      </TilePane>
   </top>
</BorderPane>
